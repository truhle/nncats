<br><br>

<%= form_for(:session, url: session_path) do |f| %>
  <%= f.label :user_name, "Username" %>
  <%= f.text_field :user_name %><br><br>

  <%= f.label :password %>
  <%= f.password_field :password %><br><br>

  <%= f.submit "Sign In" %> |
  <%= link_to "Back to Cats", cats_path %>

<% end %>
